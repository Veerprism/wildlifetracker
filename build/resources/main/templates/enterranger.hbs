<link href="enterpage.css" rel="stylesheet">
<script src="jquery-3.4.1.js"></script>

{{#partial "content"}}
    <h1>Enter you details</h1>
    <form method="post" id="enterranger" action="/verification">
        <h1>Welcome Ranger,enter your details</h1>
        <input placeholder="Username" id="rangerusername" name="rangerusername" name="id" type="text" required="">
        {{#if exists}}
        <p><strong>Username already exists</strong></p>
        {{/if}}
        <input placeholder="Password" id="rangerpassword" name="rangerpassword" name="password" type="password" required="">
        <input placeholder="Phonenumber" id="rangerphonenumber" type="text"  name="rangerphonenumber">
        <button>Submit</button>
    </form>

    <a class="follow" href="https://twitter.com/mildrenben" target="_blank"><i class="fab fa-instagram"></i>Follow Me</a>

{{/partial}}
{{> layout.hbs}}
